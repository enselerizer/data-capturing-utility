{
  "name": "example-recording",                  // имя записи
  "captureDate": "2025-01-01T00:00:00.000Z",    // время начала записи
  "experimentMetadata": {                       // данные об эксперименте
    "cuttingModesData": {                       // режимы резания
      "spindleRPM": 300,                        // скорость вращения шпинделя в об/мин
      "feedRate": 800,                          // скорость подачи в мм/мин
      "height": 1,                              // высота снятия материала за проход в мм
      "width": 5,                               // ширина снятия материала за проход в мм
      "oilMist": false,                         // включено ле охлаждение масляным туманом
      "coolantNozzle": true,                    // включено ли охлаждение водоэмульсионной СОЖ через форсунки
      "coolantSpindle": false                   // включено ли охлаждение водоэмульсионной СОЖ через микроканалы фрезы
    },
    "endmillData": {                            // данные о режущем инструменте (фрезе). Состав полей требует обсуждения
      "type": "cylindrical",                    // разновидность геометрии врезы
      "cuttingDiameter": 10,                    // диаметр режущей части
      "shankDiameter": 10,                      // диаметр хвостовика
      "cutterLength": 32,                       // высота режущей части
      "chuckLength": 49,                        // высота вылета из патрона
      "fluteConut": 3,                          // количество заходов (зубьев) на оборот
      "coating": "DLC",                         // покрытие. Перечень стандартизирован слабо, требует обсуждения
      "isoCuttingMaterials": [                  // совместимость с обрабатываемыми материалами по ISO
        "N"
      ]
    },
    "chuckData": {                              // данные о патроне
      "spindleConeType": "BT40",                // разновидность конуса (сопряжения со шпинделем)
      "colletType": "ER32",                     // разновидность цанги (сопряжения с инструменом)
      "spindleLength": "60"                     // высота между точками сопряжения со шпинделем и с инструментом
    },
    "materialData": {                           // данные об обрабатываемом материале
      "type": "metal",                          // общий тип материала
      "isoMaterialClass": "N",                  // классификация материала по ISO
      "compositionSpecByGOST": "Д16т"           // наименование сплава/стали по ГОСТ
    }
  },
  "sensors": [                                  // перечень датчиков, данные с которых присутствуют в записи
    {
      "sensorID": 0,                            // ID датчика (для привязки к файлам с данными)
      "sensorProperties": {                     // параметры датчика
        "type": "accelerometer-3axis",          // общий тип датчика
        "name": "Bosch CISS",                   // Наименование модели
        "samplingRate": 2000,                   // Частота дискретизации в Гц
        "bitDepth": 14,                         // Разрешение (битность)
        "unitName": "g",                        // единица измерения
        "maxRange": 2,                          // макс. граница диапазона измерения в указанной ед.изм.
        "minRange": -2,                         // мин. граница диапазона измерения в указанной ед.изм.
        "resolution": 0.001                     // разрешение в указанной ед.изм.
      },
      "channels": [                             // перечень каналов измерения датчика
        {
          "name": "x",                          // название канала
          "type": "int16"                       // тип данных
        },
        {
          "name": "y",
          "type": "int16"
        },
        {
          "name": "z",
          "type": "int16"
        }
      ]
    }
  ]
}
